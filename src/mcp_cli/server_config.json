{
  "mcpServers": {
    "sqlite": {
      "command": "uvx",
      "args": ["mcp-server-sqlite", "--db-path", "test.db"]
    },
    "echo": {
      "command": "uvx",
      "args": ["chuk-mcp-echo", "stdio"]
    },
    "notion": {
      "url": "https://mcp.notion.com/mcp"
    },
    "cloudflare_workers": {
      "url": "https://bindings.mcp.cloudflare.com/mcp"
    },
    "brave_search": {
      "command": "npx",
      "args": ["-y", "@brave/brave-search-mcp-server"],
      "env": {
        "BRAVE_API_KEY": "${TOKEN:bearer:brave_search}"
      }
    },
    "monday": {
      "url": "https://mcp.monday.com/mcp"
    },
    "playwright": {
      "command": "npx",
      "args": [
        "@playwright/mcp@latest"
      ]
    },
    "linkedin": {
      "url": "https://linkedin.chukai.io/mcp"
    },
    "weather": {
      "url": "https://weather.chukai.io/mcp"
    },
    "math": {
      "url": "https://math.chukai.io/mcp"
    },
    "celestial": {
      "url": "https://celestial.chukai.io/mcp"
    },
    "time": {
      "url": "https://time.chukai.io/mcp"
    },
    "physics": {
      "url": "https://physics.chukai.io/mcp"
    },
    "powerpoint": {
      "url": "https://powerpoint.chukai.io/mcp"
    },
    "motion": {
      "url": "https://motion.chukai.io/mcp"
    },
    "maritime": {
      "url": "https://maritime-archives.chukai.io/mcp"
    },
    "geocoder": {
      "url": "https://geocoder.chukai.io/mcp"
    },
    "her": {
      "url": "https://her.chukai.io/mcp"
    },
    "view_demo": {
      "url": "https://mcp-view-demo.fly.dev/mcp"
    }
  }
}